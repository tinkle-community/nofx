{
  "//": "OKX 交易配置示例 - 包含黑名单功能",
  "traders": [
    {
      "id": "okx_trader_1",
      "name": "OKX Trader with Blacklist",
      "ai_model": "deepseek",
      "exchange": "okx",
      "okx_api_key": "your_okx_api_key",
      "okx_secret_key": "your_okx_secret_key",
      "okx_passphrase": "your_okx_passphrase",
      "deepseek_key": "your_deepseek_api_key",
      "initial_balance": 1000,
      "scan_interval_minutes": 3,

      "//excluded_symbols": "黑名单：AI不会对这些币种进行交易决策（支持 Binance 或 OKX 格式）",
      "excluded_symbols": [
        "PEPE-USDT-SWAP",
        "DOGE-USDT-SWAP"
      ]
    }
  ],
  "leverage": {
    "btc_eth_leverage": 5,
    "altcoin_leverage": 5
  },
  "use_default_coins": true,
  "default_coins": [
    "BTC-USDT-SWAP",
    "ETH-USDT-SWAP",
    "SOL-USDT-SWAP",
    "BNB-USDT-SWAP"
  ],
  "coin_pool_api_url": "",
  "oi_top_api_url": "",
  "api_server_port": 8090,
  "max_daily_loss": 10.0,
  "max_drawdown": 20.0,
  "stop_trading_minutes": 60,

  "//": "说明",
  "//1": "excluded_symbols: 币种黑名单，AI 不会对这些币种进行交易决策",
  "//2": "支持两种格式：PEPEUSDT 或 PEPE-USDT-SWAP",
  "//3": "如果你已经有 PEPE 的持仓但不想让 AI 控制，就添加到黑名单",
  "//4": "黑名单币种仍会出现在持仓信息中，但AI不会对其做出交易决策"
}
